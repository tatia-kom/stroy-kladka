                    div.sales__block.sales__block--electric
                        p.sales__title Электрика
                        p.sales__text Lorem Ipsum is simply dummy text of the printing
                        button.button.button--white.open-modal(data-title="Получить скидку")
                            span Получить скидку

        section.sales
            div.container
                div.block-head
                    p.title-label Услуги
                    h2.title А также для вас действуют специальные цены от <br />наших партнеров

                div.sales__flex
                    div.sales__block.sales__block--last(style="background-image: url(img/sale3.jpg)")
                        p.sales__name Механизированная штукатурка стен
                        button.button.open-modal(data-title="Получить скидку")
                            span Получить скидку
                    div.sales__block.sales__block--last(style="background-image: url(img/sale4.jpg)")
                        p.sales__name Полусухая стяжка пола
                        button.button.open-modal(data-title="Получить скидку")
                            span Получить скидку

                div.sales__logos
                    div.sales__logo
                        img(src="img/logo1.jpg")
                    div.sales__logo
                        img(src="img/logo2.jpg")
                    div.sales__logo
                        img(src="img/logo3.jpg")
                    div.sales__logo
                        img(src="img/logo4.jpg")

        section.contacts
            div.contacts__map#map

            div.container
                div.contacts__block
                    p.contacts__title Контакты
                    address.contacts__address Химки, Ленинградская 39 стр. 6, “Химки Бизнес Парк”
                    p.contacts__label Без выходных с 9.00 до 22.00
                    a.contacts__phone(href="tel:+7 (495) 225-52-42") +7 (495) 225-52-42
                    button.button.button--form.open-modal(data-title="Заказать звонок")
                        span Заказать звонок

        footer.footer
            p Name site***2020
            a(href="") Политика конфиденциальности


